<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.pablomacias.esnuex_app">

    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/esn_star"
        android:label="@string/app_name"
        android:roundIcon="@drawable/esn"
        android:supportsRtl="true"
        android:theme="@style/NoTittleBarTheme">
        <activity
            android:name=".ui.main.activity.MainActivity"
            android:label="@string/title_home"
            android:theme="@style/NoTittleBarTheme.NoActionBar" />
        <activity android:name=".ui.login.activity.SignupActivity" />
        <activity android:name=".ui.splash.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.detail.activity.DetailActivity" />
        <activity android:name=".ui.management.activity.ManagementActivity" />
        <activity android:name=".ui.management.activity.ManagementActivityForm"></activity>
    </application>

</manifest>